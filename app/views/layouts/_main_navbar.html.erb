<ul class="no-list-style main-navbar-link-container">
  <% if logged_in? %>
    <li class="inline primary-gray">
      Welcome, <%= current_user.username %>!
    </li>
    <li class="inline main-navbar-link">
      <%= link_to 'Logout', logout_path, method: 'delete' %>
    </li>
  <% else %>
    <li class="inline main-navbar-link">
      <%= link_to "sign up", register_path, class: "link primary-gray", id: "sign_up" %>
    </li>
    <li class="inline main-navbar-link">
      <%= link_to "log in", login_path, class: "link primary-gray" %>
    </li>
  <% end %>
  <li class="inline main-navbar-link">
    <%= link_to "tour", root_path, class: "link primary-gray" %>
  </li>
  <li class="inline main-navbar-link">
    <%= link_to "help", root_path, class: "link primary-gray" %>
  </li>
  <li class="inline main-navbar-link">
    <input placeholder="search" />
  </li>
</ul>
